{
  "name": "class-components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "npm run lint"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
